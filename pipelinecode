pipeline {
    agent any
    
    stages {
        stage('one') {
            steps {
               git 'https://github.com/moulabi1911/project-1.git' 
            }
        }
        stage('artifact') {
            steps {
               sh 'mvn clean package'
            }
        }
    }
    post {
        success {
            echo 'my artifact file is generated'
        }
    }
}
